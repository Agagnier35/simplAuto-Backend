{
  "name": "simplauto-backend",
  "scripts": {
    "start": "nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "deploy": "prisma deploy --env-file .env",
    "seeds": "prisma seed --reset",
    "init-codegen": "gql-gen init",
    "generate": "gql-gen --config codegen.yml"
  },
  "dependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "10.12.18",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "6.0.0",
    "dotenv-cli": "1.4.0",
    "graphql-yoga": "1.17.0",
    "jsonwebtoken": "8.4.0",
    "nodemon": "1.18.9",
    "prisma": "1.25.3",
    "rimraf": "2.6.3",
    "ts-node": "7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "3.2.2",
    "typescript-tslint-plugin": "^0.3.1"
  },
  "devDependencies": {
    "graphql-code-generator": "^0.16.0",
    "graphql-codegen-typescript-common": "0.16.0",
    "graphql-codegen-typescript-resolvers": "^0.16.0",
    "graphql-codegen-typescript-server": "0.16.0"
  }
}
